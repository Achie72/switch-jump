function _init()
	add_enemy(10,8,1)
	add_enemy(2,8,0)
end


function camera_transition()
	while cam.x < (global.level)*16 do
		global.cameraMoving = true
		cam.x += 1
		camera(cam.x,cam.y)
	end
	if (cam.x == (global.level)*16) then
		global.cameraMoving = false
	end
end

function check_door()
	if collide_rect(to_rect(door,7,7),to_rect(player,7,7)) then
		if global.level == 7 then
			unload_level()
			global.state = 2
		else
			unload_level()
			camera_transition()
			if not global.cameraMoving then
				global.level += 1
				load_level()
			end
		end
	end
end

function unload_level()
	particles = {}
	enemies = {}
	door = {}
	bouncers = {}
	boosters = {}
end

function load_level()
	if (global.level == 1) then init_1() end
	if (global.level == 2) then init_2() end
	if (global.level == 3) then init_3() end
	if (global.level == 4) then init_4() end
	if (global.level == 5) then init_5() end
	if (global.level == 6) then init_6() end
	if (global.level == 7) then init_7() end
end

function reload_level()
	player.x = global.levelStartPosX
	player.y = global.levelStartPosY
end


